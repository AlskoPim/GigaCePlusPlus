#include <iostream>
#include <vector>
using namespace std;

int main(){}
    int li = 9;
    int spasiba = li-2;

    vector<int> n(li);

    for(int i=0; i<=n.size(); i++){
        if(i==0 || i==n.size()){
            for(int j = 0; j<n.size(); j++) cout << n[i]<< " ";
            cout << endl;
        }else{
            cout << n[i] << " ";
            for(int j = 0; j<spasiba; j++) cout << " " << " ";
            cout << n[i] << " ";
            cout << endl;
        }
    }
    return 0;
}
